model Book {
  author      String
  available   Boolean  @default(true)
  city        String
  copies      Int
  createdAt   DateTime @default(now())
  cdd         String
  edition     String
  id          String   @id @default(uuid())
  idCutter    String
  libraryId   String
  publisher   String
  releaseDate DateTime
  title       String
  tomo        Int?
  updatedAt   DateTime @updatedAt
  volume      Int?

  library Library @relation(fields: [libraryId], references: [id])

  @@index([libraryId], map: "library_libraryId_fkey")
  @@map("books")
}
