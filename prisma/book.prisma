model Book {
  author      String
  available   Boolean  @default(true)
  city        String
  copies      Int
  createdAt   DateTime @default(now())
  cdd         String
  edition     String
  id          String   @id @default(uuid())
  idCutter    String
  publisher   String
  releaseDate DateTime
  title       String
  tomo        Int?
  updatedAt   DateTime @updatedAt
  volume      Int?

  @@map("books")
}
